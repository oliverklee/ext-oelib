{
  "name": "oliverklee/oelib",
  "type": "typo3-cms-extension",
  "version": "1.1.0",
  "description": "This extension provides useful stuff for extension development: helper functions for unit testing, templating and automatic configuration checks.",
  "keywords": ["TYPO3", "extension", "oelib", "seminars", "realty", "onetimeaccount", "library"],
  "homepage": "https://www.oliverklee.de",
  "support": {
    "issues": "https://bugs.oliverklee.com/"
  },
  "authors": [
    {
      "name": "Oliver Klee",
      "role": "Developer",
      "homepage": "https://www.oliverklee.de"
    }
  ],
  "license": ["GPL-2.0+"],
  "replace": {
    "oelib": "self.version",
    "typo3-ter/oelib": "self.version"
  },
  "require": {
    "php": ">=5.5.0,<8.0",
    "typo3/cms-core": ">=6.2.0,<9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0"
  },
  "suggest": {
    "sjbr/static-info-tables": ">=6.2.0"
  },
  "autoload": {
    "classmap": ["Classes", "Tests"]
  }
}